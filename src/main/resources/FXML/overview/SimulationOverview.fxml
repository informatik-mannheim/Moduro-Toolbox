<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<ScrollPane maxHeight="2000.0" maxWidth="2000.0" minHeight="-Infinity" prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hs.mannheim.modUro.controller.overview.SimulationOverviewController">
  <content>
    <VBox maxHeight="-1.0" maxWidth="-1.0" spacing="10.0">
      <children>
        <GridPane hgap="10.0" maxHeight="-1.0" maxWidth="-1.0" prefHeight="200.0" vgap="10.0">
          <children>
            <Label text="Simulation-ID:" />
            <Label fx:id="simulationID" GridPane.columnIndex="2" />
            <Label text="Simulation-Model:" GridPane.rowIndex="1" />
            <Label fx:id="simulationModel" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Label text="Seed:" GridPane.rowIndex="2" />
            <Label fx:id="simulationSeed" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label text="Steady State:" GridPane.rowIndex="3" />
            <Label fx:id="isInSteadyState" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Label text="Completed:" GridPane.rowIndex="4" />
            <Label fx:id="isCompleted" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <Label text="Aborted:" GridPane.rowIndex="5" />
            <Label fx:id="isAborted" GridPane.columnIndex="2" GridPane.rowIndex="5" />
            <Label text="Start-time:" GridPane.rowIndex="6" />
            <Label fx:id="starttime" GridPane.columnIndex="2" GridPane.rowIndex="6" />
            <Label text="Duration:" GridPane.rowIndex="7" />
            <Label fx:id="duration" GridPane.columnIndex="2" GridPane.rowIndex="7" />
            <Label text="Directory" GridPane.rowIndex="8" />
            <Hyperlink fx:id="directoryHyperlink" onAction="#handleHyperlinkOnlicked" GridPane.columnIndex="2" GridPane.rowIndex="8" />
            <Label text="Video:" GridPane.rowIndex="10" />
            <Button fx:id="videoButton" mnemonicParsing="false" onAction="#handleImagesToVideo" text="Create Video" GridPane.columnIndex="2" GridPane.rowIndex="10" />
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="24.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
          </rowConstraints>
        </GridPane>
        <TableView fx:id="tableContent" prefHeight="-1.0" prefWidth="-1.0" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" VBox.vgrow="ALWAYS" />
        <GridPane>
          <children>
            <ImageView fx:id="firstImage" fitHeight="250.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <ImageView fx:id="secondImage" fitHeight="250.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <ImageView fx:id="thirdImage" fitHeight="250.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="0" />
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
          </rowConstraints>
        </GridPane>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>
  </content>
</ScrollPane>
