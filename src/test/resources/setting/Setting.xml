<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<moduro>
    <parameter>
        <!--
        Define how missing fitness values for aborted simulations
        should be treated.
        Values are: LAST or ZERO
        LAST: set missing vlaues to the last fitness value.
        ZERO: set missing values to 0.
        -->
        <totalfitnesstype>LAST</totalfitnesstype>
        <!-- Time in days when steady state should be reached. -->
        <steadystatetime>40</steadystatetime>
        <!-- Time in days when a completed simulations stops. -->
        <endtime>700</endtime>
        <!-- Percentage of the preview pictures -->
        <firstimage>0.2</firstimage>
        <secondimage>0.5</secondimage>
        <thirdimage>0.95</thirdimage>
        <!--
          Charactor used to extract a model name from a directory.
          This is an regular expression (for Java).
          \_ means an underscore. Other separators are: \. (dot)
          -->
        <dirnameseparator>\_</dirnameseparator>
    </parameter>
    <project>
        <name>Project1</name>
        <node>
            <name>Project1Node1</name>
            <path>src/test/resources/Simulationdata/Projekt1/node1</path>
        </node>
        <node>
            <name>Project1Node2</name>
            <path>src/test/resources/Simulationdata/Projekt1/node2</path>
        </node>
    </project>
    <project>
        <name>Project2</name>
        <node>
            <name>Project2Node1</name>
            <path>src/test/resources/Simulationdata/Projekt2/node1</path>
        </node>
    </project>
</moduro>